&i2c10 {
	status = "okay";
};

&i2c11 {
	status = "okay";
};

&i2c12 {
	status = "okay";
};

&i2c13 {
	status = "okay";

	/*
	* Fan Present/Fault 1x6 (FAN_SENSOR)
	0x40
    0x42
	*/

